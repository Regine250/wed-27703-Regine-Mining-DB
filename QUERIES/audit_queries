CREATE TABLE insert_audit (
    audit_id    NUMBER GENERATED BY DEFAULT AS IDENTITY,
    table_name  VARCHAR2(30),
    attempted_on DATE,
    attempted_by VARCHAR2(30),
    allowed      VARCHAR2(10),
    reason       VARCHAR2(200)
);

SELECT audit_id,
       table_name,
       attempted_on,
       attempted_by,
       allowed,
       reason
FROM insert_audit
ORDER BY audit_id DESC;
